<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rutListView"
        android:textSize="@dimen/tamnioLetraList"></TextView>
    <TextView
        android:textSize="@dimen/tamnioLetraList"
        android:layout_marginTop="@dimen/marginTopList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/nombreApellidoListView"></TextView>
    <TextView
        android:textSize="@dimen/tamnioLetraList"
        android:layout_marginTop="@dimen/marginTopList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/fechaListView"></TextView>
    <TextView
        android:textSize="@dimen/tamnioLetraList"
        android:layout_marginTop="@dimen/marginTopList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/areaTrabajoView"></TextView>
    <TextView
        android:textSize="@dimen/tamnioLetraList"
        android:layout_marginTop="@dimen/marginTopList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/sintomasListView"></TextView>
    <TextView
        android:textSize="@dimen/tamnioLetraList"
        android:layout_marginTop="@dimen/marginTopList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tosListView"></TextView>
    <TextView
        android:textSize="@dimen/tamnioLetraList"
        android:layout_marginTop="@dimen/marginTopList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tempListView"></TextView>
    <TextView
        android:textSize="@dimen/tamnioLetraList"
        android:layout_marginTop="@dimen/marginTopList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/presionListView"></TextView>
</LinearLayout>